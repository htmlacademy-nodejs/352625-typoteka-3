{
  "name": "typoteka",
  "version": "1.0.0",
  "description": "Типотека",
  "main": "./src/service/service.js",
  "scripts": {
    "test": "npm run eslint && npm run test::rest",
    "test::rest": "jest --runInBand",
    "eslint": "eslint --ext .js src/",
    "api": "nodemon ./src/service/service.js --server | ./node_modules/.bin/pino-pretty",
    "ssr": "nodemon ./src/express/index.js | ./node_modules/.bin/pino-pretty",
    "start": "concurrently --kill-others-on-fail --prefix \"{name}: {time}\" --names \"API,SSR\" --prefix-colors \"green,blue\" \"npm:api\" \"npm:ssr\"",
    "api:debug": "LOG_LEVEL=debug nodemon ./src/service/service.js --server | ./node_modules/.bin/pino-pretty",
    "ssr:debug": "LOG_LEVEL=debug nodemon ./src/express/index.js | ./node_modules/.bin/pino-pretty",
    "start:dev": "concurrently --kill-others-on-fail --prefix \"{name}: {time}\" --names \"API,SSR\" --prefix-colors \"green,blue\" \"npm:api:debug\" \"npm:ssr:debug\"",
    "db-connect": "node ./src/service/service.js --db-connect | ./node_modules/.bin/pino-pretty",
    "filldb": "node ./src/service/service.js --filldb $COUNT | ./node_modules/.bin/pino-pretty"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/twelwet/352625-typoteka-3.git"
  },
  "author": "Maksim Akimov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/twelwet/352625-typoteka-3/issues"
  },
  "homepage": "https://github.com/twelwet/352625-typoteka-3#readme",
  "devDependencies": {
    "eslint": "6.8.0",
    "eslint-config-htmlacademy": "0.5.2",
    "jest": "25.3.0",
    "nodemon": "2.0.2",
    "pino-pretty": "4.0.0",
    "supertest": "4.0.2"
  },
  "dependencies": {
    "@hapi/joi-date": "2.0.1",
    "axios": "0.19.2",
    "bcrypt": "5.0.0",
    "chalk": "3.0.0",
    "concurrently": "5.3.0",
    "connect-session-sequelize": "7.0.4",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-pino-logger": "4.0.0",
    "express-session": "1.17.1",
    "joi": "17.3.0",
    "moment": "2.24.0",
    "multer": "1.4.2",
    "nanoid": "2.1.11",
    "pg": "8.3.3",
    "pino": "6.2.0",
    "pug": "2.0.4",
    "sequelize": "6.3.5"
  }
}
